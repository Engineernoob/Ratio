  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "@supabase/supabase-js": "^2.86.0",
    "openai": "^4.20.0",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20",
    "@types/node-cron": "^3.0.2",
    "tsx": "^4.7.0",
    "typescript": "^5"
  }
